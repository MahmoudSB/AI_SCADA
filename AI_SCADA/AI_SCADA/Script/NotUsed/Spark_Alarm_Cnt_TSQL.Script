    myCmd.CommandType = System.Data.CommandType.Text
    myCmd.CommandText = Qry
    myCmd.ExecuteNonQuery

    myReader = myCmd.ExecuteReader()
    RowCount = 0
    While myReader.Read()
        For i = 0 To myReader.FieldCount -1
            Num = myReader.GetValue(i)
