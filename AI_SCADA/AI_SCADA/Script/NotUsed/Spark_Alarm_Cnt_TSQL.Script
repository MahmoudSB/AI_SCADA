Sub TSQL_Counter(itr As Integer, Tbl As String, Whr As String)
    
    Dim Qry As String= "Select count(DISTINCT [EventMessage]) from [" &amp; Tbl &amp; "] " &amp; Whr

    Dim myCmd As New System.Data.SqlClient.SqlCommand
